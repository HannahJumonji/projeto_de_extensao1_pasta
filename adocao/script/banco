CREATE DATABASE boladepelo;

-- Tabela Animal
CREATE TABLE animal (
id SERIAL PRIMARY KEY,
nome VARCHAR(100) NOT NULL,
idade VARCHAR(50),
descricao TEXT,
adotado BOOLEAN DEFAULT FALSE,
foto_url TEXT,
ong VARCHAR(100),
contato VARCHAR(100)
);


-- Exemplo de inserção de teste
INSERT INTO animal (nome, idade, descricao, foto_url, ong, contato) VALUES
('Mingau', '2 anos', 'Gato carinhoso e castrado', 'data:image/jpeg;base64,', 'Bola de Pelo', '+559999999999'),
('Bolinha', '1 ano', 'Gato brincalhão e sociável', 'data:image/jpeg;base64,', 'Bola de Pelo', '+559999999999');